# AxeOS Agent Configuration
# These can also be set via the config file at ~/.axeos-agent/config.json

# API server URL (for setup/pairing)
AXEOS_API_URL=http://localhost:3000/api/v1

# WebSocket server URL (for metrics streaming)
AXEOS_SERVER_URL=ws://localhost:3001/ws

# Metrics collection interval in milliseconds
AXEOS_METRICS_INTERVAL=5000
