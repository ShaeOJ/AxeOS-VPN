{
  "name": "@axeos-vpn/desktop",
  "version": "1.7.3",
  "description": "Vault-Tec Mining Operations Division - BitAxe Monitoring System",
  "author": "Vault-Tec Industries <shae@undergroundroots.ca>",
  "repository": {
    "type": "git",
    "url": "https://github.com/ShaeOJ/AxeOS-VPN.git"
  },
  "private": true,
  "main": "./dist/main/index.js",
  "scripts": {
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "preview": "electron-vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "clean": "rimraf dist out",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "dist:win": "electron-builder --win",
    "dist:mac": "electron-builder --mac",
    "dist:linux": "electron-builder --linux"
  },
  "dependencies": {
    "@axeos-vpn/shared-types": "workspace:*",
    "@axeos-vpn/shared-utils": "workspace:*",
    "@electron-toolkit/utils": "^3.0.0",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^9.2.2",
    "bindings": "^1.5.0",
    "body-parser": "^2.2.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "file-uri-to-path": "^2.0.0",
    "jsonwebtoken": "^9.0.2",
    "qrcode.react": "^4.2.0",
    "ws": "^8.16.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/better-sqlite3": "^7.6.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/ws": "^8.5.10",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "electron-vite": "^2.0.0",
    "postcss": "^8.4.32",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.0",
    "recharts": "^2.10.3",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.10"
  },
  "build": {
    "appId": "com.axeos.vpn",
    "productName": "AxeOS VPN Monitor",
    "publish": null,
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "package.json"
    ],
    "asar": true,
    "asarUnpack": [
      "**/*.node",
      "**/better-sqlite3/**/*"
    ],
    "nodeGypRebuild": false,
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "resources/icon.ico",
      "artifactName": "AxeOS-VPN-Monitor-${version}-Setup.${ext}"
    },
    "nsis": {
      "guid": "AxeOS-VPN-Monitor-8F3D9C2A",
      "oneClick": false,
      "perMachine": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "AxeOS VPN Monitor",
      "runAfterFinish": false
    },
    "mac": {
      "target": [
        {
          "target": "dmg",
          "arch": ["x64", "arm64"]
        },
        {
          "target": "zip",
          "arch": ["x64", "arm64"]
        }
      ],
      "artifactName": "AxeOS-VPN-Monitor-${version}-macOS-${arch}.${ext}",
      "category": "public.app-category.utilities",
      "minimumSystemVersion": "10.15"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "icon": "resources/icon.png",
      "artifactName": "AxeOS-VPN-Monitor-${version}-Linux.${ext}",
      "category": "Utility"
    }
  }
}
